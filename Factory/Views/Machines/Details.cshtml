<h2>Engineer Details</h2>

<div>
  <h4>@Model.Name</h4>
  <hr />
  <dl class="row">
    <dt class="col-sm-2">
      Machines Licensed to Repair:
    </dt>
    <dd class="col-sm-10">
      @foreach (var em in Model.Machines)
      {
        <div>@em.Machine.Name</div>
      }
    </dd>
  </dl>
</div>
<div>
  <a asp-action="Edit" asp-route-id="@Model.EngineerId">Edit</a> |
  <a asp-action="Delete" asp-route-id="@Model.EngineerId">Delete</a> |
  <a asp-action="Index">Back to list</a>
</div>